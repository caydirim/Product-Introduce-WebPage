{
  "expo": {
    "name": "Wind Tunnel App",
    "slug": "wind-tunnel-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.windtunnel.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.windtunnel.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "build": {
        "babel": {
          "include": ["@react-native-community/slider"]
        }
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "windtunnel",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes"
  }
}
